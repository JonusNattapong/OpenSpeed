{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "benchmarks/dist",
    "rootDir": "benchmarks/apps"
  },
  "include": ["benchmarks/apps/**/*"]
}